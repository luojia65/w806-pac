              <bitOffset>1</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-only</access>
              <enumeratedValues> 
                <name>ResetState</name>
                <enumeratedValue>
                  <name>has_reset</name>
                  <description>Reset has procceeded.</description>
                  <value>1</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>no_reset</name>
                  <description>No reset have been procceeded.</description>
                  <value>0</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>watchdog</name>
              <description>Reads if watchdog has been software reset.</description>
              <bitOffset>0</bitOffset>
              <bitWidth>1</bitWidth>
              <access>read-only</access>
              <enumeratedValues derivedFrom="ResetState" /> 
            </field>
          </fields>
        </register>
