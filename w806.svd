                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
        </register>
        <register>
          <name>reset_state</name>
          <description>CPU and watchdog reset state register</description>
          <addressOffset>0x001C</addressOffset>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x0003ffff</resetMask>
